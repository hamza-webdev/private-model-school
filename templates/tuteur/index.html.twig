{% extends 'base.html.twig' %}

{% block title %}Tuteur index{% endblock %}

{% block body %}
    <h1>Tuteur index</h1>

    <table class="table">
        <thead>
            <tr>
                <th class="text-center">Id</th>
                <th class="text-center">Name_father</th>
                <th class="text-center">Name_mather</th>
                <th class="text-center">Telephone_father</th>
                <th class="text-center">Telephone_mother</th>
                <th class="text-center">Email</th>
                <th class="text-center">Password</th>
                <th class="text-center">Adresse</th>
                <th class="text-center">City</th>
                <th class="text-center">actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tuteur in tuteurs %}
            <tr>
                <td>{{ tuteur.id }}</td>
                <td>{{ tuteur.nameFather }}</td>
                <td>{{ tuteur.nameMather }}</td>
                <td>{{ tuteur.telephoneFather }}</td>
                <td>{{ tuteur.telephoneMother }}</td>
                <td>{{ tuteur.email }}</td>
                <td>{{ tuteur.password }}</td>
                <td>{{ tuteur.adresse }}</td>
                <td>{{ tuteur.city }}</td>
                <td>
                    <a href="{{ path('tuteur_show', {'id': tuteur.id}) }}">show</a>
                    <a href="{{ path('tuteur_edit', {'id': tuteur.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('tuteur_new') }}">Create new</a>
{% endblock %}
