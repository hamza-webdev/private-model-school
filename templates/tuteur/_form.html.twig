{{ form_start(form) }}

    {{ form_row(form.name_father)}}
    {{ form_row(form.name_mather)}}
    {{ form_row(form.telephone_father)}}
    {{ form_row(form.telephone_mother)}}
    {{ form_row(form.email)}}
    {{ form_row(form.password)}}
    {{ form_row(form.adresse)}}
    {{ form_row(form.city)}}

    <button:button class="btn btn-primary btn-new"
        id="new-student"
        data-collection=#students

        >New Student</button:button>

    <div id="students" class="row"
        data-prototype="{{ include("tuteur/_student.html.twig", {form: form.students.vars.prototype})|e("html_attr")}}"
        data-index="{{form.students|length > 0 ? form.students|last.vars.name +1 : 0}} ">
    {% for student in form.students  %}
        {{ include("tuteur/_student.html.twig", {form: student} )}}

    {% endfor %}

    </div>

